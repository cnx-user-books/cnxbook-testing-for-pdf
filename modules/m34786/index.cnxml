<m:mn>8</m:mn>
<m:mo>+</m:mo>
<m:mn>9</m:mn>
<m:mo>+</m:mo>
<m:mn>8</m:mn>
<m:mo>+</m:mo>
<m:mn>6</m:mn>
<m:mo>=</m:mo>
<m:mn>33</m:mn>
</m:mrow>
</m:mtd>
<m:mtd>
</m:mtd>
<m:mtd columnalign="left">
<m:mtext>Write 3, carry the 3.</m:mtext>

<m:mo>+</m:mo>
<m:mn>1</m:mn>
<m:mo>+</m:mo>
<m:mn>0</m:mn>
<m:mo>+</m:mo>
<m:mn>5</m:mn>
<m:mo>+</m:mo>
<m:mn>1</m:mn>
<m:mo>=</m:mo>
<m:mn>8</m:mn>
</m:mrow>
</m:mtd>
<m:mtd>
</m:mtd>
<m:mtd columnalign="left">
<m:mtext>Write 8.</m:mtext>
</m:mtd>
</m:mtr>
<m:mtr>
<m:mtd columnalign="left">
<m:mrow>
<m:mn>0</m:mn>
<m:mo>+</m:mo>
<m:mn>9</m:mn>
<m:mo>+</m:mo>
<m:mn>9</m:mn>
<m:mo>+</m:mo>
<m:mn>8</m:mn>

<m:mn>5</m:mn>
<m:mo>+</m:mo>
<m:mn>1</m:mn>
<m:mo>+</m:mo>
<m:mn>7</m:mn>
<m:mo>=</m:mo>
<m:mn>19</m:mn>
</m:mrow>
</m:mtd>
<m:mtd>
</m:mtd>
<m:mtd columnalign="left">
